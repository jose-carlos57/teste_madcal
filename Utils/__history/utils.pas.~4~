function capturaClick(RadioAtivo: TRadioButton; RadioInativo:TRadioButton; var status: Integer);
begin
  if RadioAtivo.Checked  then
      status := 1
  else if RadioInativo.Checked then
      status := 0
  else
    Result :=

end;
end.
